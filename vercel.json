{
    "version": 2,
    "builds": [
      {
        "src": "server.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "/api/v1/ask",
        "dest": "server.ts"
      },
      {
        "src": "/api/v1/(.*)",
        "dest": "server.ts"
      },
      {
        "src": "/(.*)",
        "dest": "server.ts"
      }
    ]
  }
  